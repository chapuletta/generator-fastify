{
  "name": "generator-fastify",
  "version": "1.0.7",
  "description": "Fastify Generator for APIs",
  "homepage": "https://github.com/nbalduzzi",
  "author": {
    "name": "Nicolas Balduzzi",
    "email": "nico.balduzzi@gmail.com",
    "url": "https://github.com/nbalduzzi"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["yeoman-generator", "fastify", "archetype"],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.22.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "nsp": "^3.2.1",
    "prettier": "^1.12.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.2"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.5",
    "chalk": "^2.4.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nbalduzzi/generator-fastify.git"
  }
}
